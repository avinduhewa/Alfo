{
  "name": "propertieslk-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon",
    "start": "node -r module-alias/register ./dist --env=developement",
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "test": "jest --testTimeout=10000",
    "sync-with-db": "npx prisma introspect && npx prisma generate",
    "sync-to-db": "npx prisma migrate dev"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "src/daos",
    "@entities": "src/entities",
    "@shared": "src/shared",
    "@server": "src/Server",
    "@controller": "src/controllers",
    "@interfaces": "src/interfaces",
    "@models": "src/models"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/find": "^0.2.1",
    "@types/helmet": "^0.0.48",
    "@types/jsonfile": "^6.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.18",
    "@types/node": "^15.12.2",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/tedious": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "child_process": "^1.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.22.0",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "git-cz": "^4.7.6",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "prisma": "^2.25.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@prisma/client": "^2.25.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.12.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "tedious": "^11.0.9"
  }
